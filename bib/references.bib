	journal = {American Journal of Roentgenology},
	author = {Meier, Andreas and Alkadhi, Hatem},
	month = jul,
	year = {2019},
	note = {Publisher: American Roentgen Ray Society},
	keywords = {superior thoracic inlet venous obstruction, venous collateral pathways},
	pages = {200--210},
	file = {Full Text PDF:/home/arcotpixel/Zotero/storage/4EMRR2K4/Meier and Alkadhi - 2019 - Venous Collateral Pathways in Superior Thoracic In.pdf:application/pdf},
}

