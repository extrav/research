	urldate = {2022-07-28},
	journal = {Journal of Neurology, Neurosurgery, and Psychiatry},
	author = {Zamboni, P and Galeotti, R and Menegatti, E and Malagoni, A M and Tacconi, G and Dall’Ara, S and Bartolomei, I and Salvi, F},
	month = apr,
	year = {2009},
	pmid = {19060024},
	pmcid = {PMC2647682},
	pages = {392--399},
	file = {Full Text:/home/arcotpixel/Zotero/storage/5RGVSD2J/Zamboni et al. - 2009 - Chronic cerebrospinal venous insufficiency in pati.pdf:application/pdf},
}

